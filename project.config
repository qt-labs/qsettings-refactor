[access]
	inheritFrom = All-Non-CI-Projects
[receive]
	requireContributorAgreement = true
	requireChangeId = true
[submit]
	action = cherry pick
	mergeContent = true
[access "refs/*"]
	owner = group Administrators
[project]
	description = git clone ssh://codereview.qt-project.org/qt-labs/qsettings-refactor.git
[cherryPick]
	includeReviewedOn = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
